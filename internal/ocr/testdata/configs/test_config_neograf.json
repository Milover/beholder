{
	"ocr": {
		"config_paths": [
		],
		"model": "internal/ocr/_models/dotmatrix/dotOCRDData1.traineddata",
		"page_seg_mode": "single_line",
		"variables": {
			"load_system_dawg": "0",
			"load_freq_dawg": "0",
			"tessedit_char_whitelist": "V0123456789"
		},
		"patterns": [
			"\\A\\d\\d\\d\\d\\d\\d\\d\\d"
		]
	},
	"image_processing": {
		"preprocessing": [
			{
				"auto_crop": {
					"kernel_size": 35,
					"text_width": 50,
					"text_height": 50,
					"padding": 10
				}
			},
			{
				"resize": {
					"width": 205,
					"height": 34
				}
			},
			{
				"normalize_brightness_contrast": {
					"clip_pct": 1.5
				}
			},
			{
				"threshold": {
					"value": 0,
					"max_value": 255,
					"type": [ "binary", "otsu" ]
				}
			},
			{
				"gaussian_blur": {
					"kernel_width": 3,
					"kernel_height": 5,
					"sigma_x": 0,
					"sigma_y": 0
				}
			},
			{
				"equalize_histogram": null
			},
			{
				"morphology": {
					"kernel_type": "rectangle",
					"kernel_width": 3,
					"kernel_height": 3,
					"type": "dilate",
					"iterations": 1
				}
			}
		],
		"postprocessing": [
		]
	}
}
